<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Theme</title>

  <%= csrf_meta_tags %>

  <!-- Main css styles -->
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>

  <!-- Main javascript files -->
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

</head>
<body>
  <div id="wrapper">
    <%= render 'layouts/theme/bootstrap/inspinia/navigation' %>
    <div id="page-wrapper" class="gray-bg">
      <%= render 'layouts/theme/bootstrap/inspinia/topnavbar' %>
      <%= yield %>
      <%= render 'layouts/theme/bootstrap/inspinia/footer' %>
    </div>
  </div>
</body>
<%= flash_messages %>
</html>
